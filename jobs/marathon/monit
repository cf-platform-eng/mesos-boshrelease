check process marathon
  with pidfile /var/vcap/sys/run/marathon/marathon.pid
  start program "/var/vcap/packages/bosh-helpers/monit_debugger marathon_ctl '/var/vcap/jobs/marathon/bin/marathon_ctl start'"
  stop program "/var/vcap/packages/bosh-helpers/monit_debugger marathon_ctl '/var/vcap/jobs/marathon/bin/marathon_ctl stop'"
  group vcap
